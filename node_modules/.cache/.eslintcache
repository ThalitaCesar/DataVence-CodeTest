',\n    'Ficção Cientifica',\n    'Romance Policial',\n    'Aventura',\n    'Literatura Clássica',\n    'Grandes Poetas',\n    'Arquitetura e Design',\n    'Autodesenvolvimento',\n    'Carreira',\n    'Política'\n];\n\nconst Form : React.FC = () => {\n    const {viaCep} = useContext(GlobalContext);\n    const [userCep,\n        setUserCep] = useState < ViaCep | null > (viaCep);\n    const [open,\n        setOpen] = useState(false);\n    const [openError, setOpenError] = useState(false);\n    const [additionalInfoFile,\n        setAdditionalInfoFile] = useState < File | null > (null);\n    const [data,\n        setData] = useState < FormValues > ({\n        name: '',\n        cpf: '',\n        email: '',\n        neighborhood: `${userCep\n            ?.bairro}`,\n        cep: `${userCep\n            ?.cep}`,\n        city: `${userCep\n            ?.localidade}`,\n        street: `${userCep\n            ?.logradouro}`,\n        complement: '',\n        state: `${userCep\n            ?.uf}`,\n        number: '',\n        plan: '',\n        themes: [],\n        additionalInfo: ''\n    });\n\n    const {handleSubmit, control, formState: {\n            errors\n        }, setValue} = useForm < FormValues > ({resolver: yupResolver(validationSchema)});\n\n    useEffect(() => {\n        if (userCep) {\n            setValue('cep', userCep.cep);\n            setValue('neighborhood', userCep.bairro);\n            setValue('city', userCep.localidade);\n            setValue('street', userCep.logradouro);\n            setValue('state', userCep.uf);\n        }\n    }, [userCep, setValue]);\n\n    const handleFormSubmit : SubmitHandler < FormValues > = async(data) => {\n        try {\n            await axios.post('http://api.webhookinbox.com/i/c9652yJP/in/', data);\n            setOpen(true);\n            setOpenError(false);\n        } catch (error) {\n            setOpenError(true);\n            setOpen(false);\n        }\n\n        console.log(data);\n    };\n\n    const useStyles